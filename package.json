{
  "name": "rhapi-rdv",
  "version": "0.1.0",
  "homepage": "{homepage}",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhapi-project/rhapi-rdv.git"
  },
  "license": "MIT",
  "maintainers": [
    {
      "name": "Lambdasoft",
      "email": "contact@lambdasoft.fr"
    }
  ],
  "dependencies": {
    "fullcalendar": "^3.9.0",
    "jquery-ui": "^1.12.1",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-color": "^2.14.1",
    "react-dates": "^16.6.1",
    "react-dom": "^16.3.2",
    "react-iframe": "^1.1.0",
    "react-simple-timefield": "^2.0.0",
    "rhapi-client": "^1.0.13",
    "semantic-ui-css": "2.2",
    "semantic-ui-react": "^0.80.0"
  },
  "scripts": {
    "start": "npm run iframes && react-scripts start",
    "build": "npm run iframes; react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --write src/components/* src/css/* src/index.js",
    "docs": "markdown-folder-to-html; rm -fr public/docs; mv _docs public/docs;",
    "os": "node -e require('os').platform()",
    "iframes": "npm run os && node -e os.platform() === 'win*' ? xcopy '.\\node_modules\\semantic-ui-css\\*' '.\\public\\iframes\\css\\semantic-ui-css\\' : cp -fr ./node_modules/semantic-ui-css ./public/iframes/css"
  },
  "devDependencies": {
    "markdown-folder-to-html": "^1.6.0",
    "prettier": "^1.12.1",
    "react-scripts": "^1.1.4"
  }
}
